<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Button colors -->
    <SolidColorBrush x:Key="BtnDisForeground" Color="{StaticResource Gray950}"/>
    <SolidColorBrush x:Key="BtnDisBackground" Color="{StaticResource Gray200}"/>
    <SolidColorBrush x:Key="BtnForeground" Color="{StaticResource White}"/>
    <SolidColorBrush x:Key="BtnBackground" Color="{StaticResource Primary}"/>

    <!-- Sidebar colors -->
    <SolidColorBrush x:Key="SidebarBackground" Color="White"/>
    <SolidColorBrush x:Key="SidebarItemColor" Color="#7b8792"/>
    <SolidColorBrush x:Key="SidebarSlcItemColor" Color="#2a84f1"/>
    
    <!-- Frame colors -->
    <SolidColorBrush x:Key="FrameBackground" Color="{StaticResource Gray100}"/>
</ResourceDictionary>